pipeline {
  agents any
    stages {
      stage ('build') {
        steps {
               sh '''git branch: 'main', url: 'https://github.com/madhavivijay/hello-world-war.git
               mvn clean install'''
              }
             }
       stage ('deploy') {
        steps { 
            echo "deploy"
            }
          }           
    }
}
