pipeline {
  agent any
    stages {
      stage ('build') {
        steps {
               sh '''git branch: 'main', url: 'https://github.com/madhavivijay/hello-world-war.git
               mvn clean package
               '''
              }
             }
       stage ('deploy') {
        steps { 
            echo "this is deploy stage"
            }
          }           
    }
}
