pipeline {
  agents any
    stages {
      stage ('build') {
        steps {
               git branch: 'main', url: 'https://github.com/madhavivijay/hello-world-war.git
               sh 'mvn clean install'
              }
             }
       stage ('deploy') {
        steps { 
            echo "deploy"
            }
          }           
    }
}
